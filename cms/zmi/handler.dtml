<dtml-var manage_page_header>
<dtml-var manage_tabs>

<dtml-with "handler_map()" mapping>
  <h3>handler: <dtml-var repr html_quote></h3>
  <table>
    <dtml-in dict mapping>
      <tr>
        <th valign="top" align="right"><dtml-var "name"></th>
        <td valign="top"><dtml-var "value" html_quote></td>
      </tr>
    </dtml-in>
  </table>
</dtml-with>


<dtml-var manage_page_footer>